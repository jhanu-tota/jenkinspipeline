pipeline {
  agent any
  stages {
    stage('Development') {
      steps {
        echo 'i want to develop'
      }
    }

    stage('Build') {
      steps {
        echo 'i want to build'
      }
    }

    stage('Test') {
      steps {
        echo 'i want to test'
      }
    }

    stage('deploy') {
      steps {
        echo 'i want to deploy'
      }
    }

  }
}